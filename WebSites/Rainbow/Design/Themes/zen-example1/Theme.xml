	<Parts>
		<ThemePart Name="ModuleLayout">
			<HTML><![CDATA[
				<div class="zm">
					<div class="zm-hdr">
					<if:Title>
						<div class="zm-content">
							<h2>
								<content:TitleText />
							</h2>
						</div>
					</if:Title>
					<if:Buttons>
						<div class="zm-ctrl">
							<ul>
								<loop:Buttons>
									<li>
										<content:Button />
									</li>
								</loop:Buttons>
							</ul>
						</div>
					</if:Buttons>
				</div>
					<if:Body>
					<div class="zm-bdy">
						<div class="zm-content">
							<content:ModuleContent />
						</div>
					</div>
				</if:Body>
					<if:Footer>
					<div class="zm-ftr">
						<div class="zm-content">
							<if:ShowModifiedBy>
								<content:ModifiedBy />
							</if:ShowModifiedBy>
						</div>
					</div>
				</if:Footer>
				</div> ]]>
			</HTML>
		</ThemePart>
